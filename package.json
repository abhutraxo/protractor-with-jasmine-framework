{
  "name": "crossover-profile-pages",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "webdriver-manager update --gecko=false",
    "js:lint": "eslint --ignore-path .gitignore .",
    "clean": "rm -rf auto-generated coverage dist e2e-report *.log",
    "clean:all": "npm run clean -- node_modules",
    "build": "tsc -p ./",
    "lint": "tslint  --project tsconfig.json --config tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "validate": "npm-run-all --parallel build lint js:lint",
    "e2e": "protractor protractor.conf.js",
    "e2e:ci": "protractor protractor.local.conf.js",
    "e2e:bs": "protractor protractor.browserstack.conf.js",
    "e2e:bsSerial": "protractor protractor.browserstack.serial.conf.js",
    "e2e:report": "allure generate auto-generated/allure-results -o e2e-report --clean",
    "e2e:rerun": "protractor protractor.rerun.conf.js",
    "e2e:bsSerialRerun": "protractor protractor.browserstack-serial.rerun.conf.js",
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/compiler": "^6.1.8",
    "@angular/core": "^6.1.8",
    "allure-commandline": "^2.5.0",
    "allure-env-properties-adder": "^1.0.8",
    "await-handler": "^1.1.2",
    "browserstack-local": "^1.3.0",
    "jasmine": "^3.5.0",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-reporters": "^2.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "log4js": "^3.0.5",
    "moment": "^2.23.0",
    "node-vibrant": "^3.0.0",
    "protractor": "^7.0.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.2",
    "shortid": "^2.2.8",
    "standard": "^12.0.1",
    "ts-simple-nameof": "^1.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.11.3",
    "codelyzer": "~4.4.4",
    "eslint-config-strongloop": "^2.1.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.2.1",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "protractor-parallel-instances": "^1.0.15",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-jasmine-rules": "^1.5.1",
    "typescript": "~3.1.1",
    "json-typescript-mapper": "1.1.3",
    "json-object-mapper": "1.6.0",
    "ts-md5": "1.2.7",
    "request-promise-native": "1.0.8",
    "@types/request-promise-native": "1.0.14",
    "unirest": "0.6.0",
    "xmlhttprequest": "1.8.0"
  }
}
